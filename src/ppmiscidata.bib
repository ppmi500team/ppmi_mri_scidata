@article{das_ic-02-05_2011,
	title = {{IC}-02-05: {MRI}-based biomarkers from hippocampal subfields: {Advances} in automatic labeling, longitudinal atrophy estimation and postmortem anatomical atlas},
	issn = {1552-5279},
	doi = {10.1016/j.jalz.2011.05.012},
	abstract = {Background: We present an image analysis framework geared towards development of focal MRI biomarkers based on hippocampal subfields. This includes: 1) An automatic segmentation method that can reliably label hippocampal subfields, with accuracy comparable to manual labeling protocols (Neuroimage, 53:1208), 2) An automated analysis pipeline for measuring longitudinal atrophy rates within subfields, and 3) A detailed computational atlas of the human hippocampus constructed from MRI and histopathological staining of autopsy samples (Neuroimage, 50:434). We present preliminary studies using this framework. Methods: Focal T2-weighted MRI (HFMRI) were obtained with high resolution slices (0.5x0.6x2.6 mm3 for cross-sectional and 0.4x0.4x2 mm3 for longitudinal cohort), oriented orthogonal to the main axis of the hippocampus (see figure). We labeled subfields using our automatic segmentation technique, that uses a multiatlas based label fusion approach, and computed their normalized volume and thickness. Anisotropic resolution makes normalization (Figure presented) of baseline and 1-3 year follow up HFMRI challenging for longitudinal atrophy estimation.We align a small ROI around the hippocampus, providing better accuracy than whole brain normalization, followed by our unbiased deformation based morphometry strategy (Neuroimage, 50:434). In the cross-sectional study, CA1 had greater thickness in controls compared to MCI patients. Left CA1, DG, and whole hippocampus had most significant volume differences. In the longitudinal study, CA1 and HEAD had significant tissue shrinkage only in the patient group, which were also significantly greater than controls. The computational atlas has been enhanced using additional specimens undergoing both histological staining MRI scans. This will improve subfield labeling accuracy, and help label additional ROIs, e.g. perirhinal and postrhinal cortices. Conclusions: All software and the atlas are being made publicly available as development continues. The automated segmentation methodology lays the foundation for a cost-effective processing pipeline for in vivo data in large clinical studies where manual segmentation is prohibitively expensive. This technology enables group studies, including longitudinal studies, that require ROI labels. Further, we have attempted to address the challenges of measuring longitudinal change from HFMRI. We anticipate that enhancement of the atlas through histology data and refinement of analysis methods will reveal greater subfield- specific effects in larger datasets.},
	journal = {Alzheimer's \& Dementia},
	author = {Das, Sandhitsu and Avants, Brian and Kadivar, Salmon and Mueller, Susanne and Pluta, John and Orozco, Sylvia and Wang, Hongzhi and Weiner, Michael and Wolk, David and Yushkevich, Paul},
	year = {2011},
}

@article{shen_sparse_2008,
	title = {Sparse principal component analysis via regularized low rank matrix approximation},
	issn = {0047259X},
	doi = {10.1016/j.jmva.2007.06.007},
	abstract = {Principal component analysis (PCA) is a widely used tool for data analysis and dimension reduction in applications throughout science and engineering. However, the principal components (PCs) can sometimes be difficult to interpret, because they are linear combinations of all the original variables. To facilitate interpretation, sparse PCA produces modified PCs with sparse loadings, i.e. loadings with very few non-zero elements. In this paper, we propose a new sparse PCA method, namely sparse PCA via regularized SVD (sPCA-rSVD). We use the connection of PCA with singular value decomposition (SVD) of the data matrix and extract the PCs through solving a low rank matrix approximation problem. Regularization penalties are introduced to the corresponding minimization problem to promote sparsity in PC loadings. An efficient iterative algorithm is proposed for computation. Two tuning parameter selection methods are discussed. Some theoretical results are established to justify the use of sPCA-rSVD when only the data covariance matrix is available. In addition, we give a modified definition of variance explained by the sparse PCs. The sPCA-rSVD provides a uniform treatment of both classical multivariate data and high-dimension-low-sample-size (HDLSS) data. Further understanding of sPCA-rSVD and some existing alternatives is gained through simulation studies and real data examples, which suggests that sPCA-rSVD provides competitive results. © 2007 Elsevier Inc. All rights reserved.},
	journal = {Journal of Multivariate Analysis},
	author = {Shen, Haipeng and Huang, Jianhua Z.},
	year = {2008},
	keywords = {Singular value decomposition, Thresholding, Dimension reduction, High-dimension-low-sample-size, Regularization},
}

