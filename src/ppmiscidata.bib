}

@article{carmichael_joint_2017,
	title = {{JOINT} {ANALYSIS} {OF} {H}\&{E} {STAINED} {IMAGES} {AND} {GENETIC} {COVARIATES} {USING} {CONVOLUTIONAL} {NEURAL} {NETWORKS} {AND} {AJIVE} ({VERSION} 1.0, 10/10/19)},
	issn = {19417330},
	abstract = {A key challenge in modern data analysis is understanding con- nections between complex and differing modalities of data. For ex- ample, there are two main approaches to the study of breast cancer: histopathology (analyzing visual characteristics of tumor biopsies) and genetics. While histopathology is the gold standard for diagnos- tics and there have been many recent breakthroughs in genetics, there is little overlap between these two fields. We aim to bridge this gap by developing methods based on Angle-based Joint and Individual Variation Explained to compare and contrast data from histopathol- ogy and genetics. Quantitative analysis of histopathology data re- quires statistical analysis of images and presents a variety of chal- lenges including feature extraction. Convolutional neural networks (CNNs) have proven adept at solving predictive tasks with images and are believed to be powerful, automatic feature extraction algo- rithms. Leveraging deep learning for exploratory/inferential analysis, where the goal is for humans to learn about patterns in the data, is challenging because deciphering signals captured by deep learning algorithms is a notoriously difficult problem. We therefore develop methods for interpreting visual signals discovered in the data when doing exploratory analysis using CNNs for image feature extraction. Finally, we give new results about the subspace geometry underlying many multi-block data algorithms. 1.},
	journal = {arXiv Preprint},
	author = {Carmichael, Iain and Calhoun, Bejamin and Hoadley, Katherine A. and Troester, Melissa and Geradts, Joseph and Olsson, Linnea and Hannih, Jan and Maron, J.S.},
	year = {2017},
	note = {\_eprint: 0000.0000},
}

