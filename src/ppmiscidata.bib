@article{weberReproducibilityFunctionalNetwork2013,
	title = {Reproducibility of functional network metrics and network structure: {A} comparison of task-related {BOLD}, resting {ASL} with {BOLD} contrast, and resting cerebral blood flow},
	issn = {15307026},
	doi = {10.3758/s13415-013-0181-7},
	abstract = {Network analysis is an emerging approach to functional connectivity in which the brain is construed as a graph and its connectivity and information processing estimated by mathematical characterizations of graphs. There has been little to no work examining the reproducibility of network metrics derived from different types of functional magnetic resonance imaging (fMRI) data (e.g.; resting vs. task related, or pulse sequences other than standard blood oxygen level dependent [BOLD] data) or of measures of network structure at levels other than summary statistics. Here, we take up these questions, comparing the reproducibility of graphs derived from resting arterial spin-labeling perfusion fMRI with those derived from BOLD scans collected while the participant was performing a task. We also examine the reproducibility of the anatomical connectivity implied by the graph by investigating test-retest consistency of the graphs' edges. We compare two measures of graph-edge consistency both within versus between subjects and across data types. We find a dissociation in the reproducibility of network metrics, with metrics from resting data most reproducible at lower frequencies and metrics from task-related data most reproducible at higher frequencies; that same dissociation is not recapitulated, however, in network structure, for which the task-related data are most consistent at all frequencies. Implications for the practice of network analysis are discussed. © 2013 Psychonomic Society, Inc.},
	journal = {Cognitive, Affective and Behavioral Neuroscience},
	author = {Weber, Matthew J. and Detre, John A. and Thompson-Schill, Sharon L. and Avants, Brian B.},
	year = {2013},
	pmid = {23813017},
	keywords = {Reproducibility, fMRI, Functional connectivity, Network analysis, Perfusion},
}

@inproceedings{bolineCreatingINCFDigital2012,
	title = {Creating {INCF} digital atlas hubs through registration workflows},
	abstract = {The International Neuroinformatics Coordinating Facility (INCF, http://incf.org/) Digital Atlasing Program is working towards a digital atlasing framework that will make multidimensional data of the rodent brain more widely accessible and usable to the research community (Hawrylycz et al, 2011, PLoS Comput Biol 7[2]: e1001065. doi:10.1371/journal.pcbi.1001065). The framework requires standard spatial reference systems (principally Waxholm Space, WHS) and a supporting data sharing infrastructure (Digital Atlasing Infrastructure, DAI); each of which is the main focus of an Atlasing Task Force. User data are primarily shared through atlas hubs, specialized servers available as part of the infrastructure or that may be installed and managed locally. Hubs register users' data to a spatial reference system, publish the data and services with INCF Central and share data via web services. At this time, the focus of both groups is on integrating image registration workflows to the underlying infrastructure with the aid of metadata, provenance, and ontology standards. Data sharing use cases drive our workflow development. At this time, we are focusing on 2D brain slice images (sampling varies from sparse to densely sampled). The goal is to create tools, recommendations, and standard operating procedures to aid in the registration of data to a known standard atlas space and creation of new atlas hubs. With workflows that allow the creation of atlas hubs, new datasets will be able to be queried and compared to data in other atlas hubs, such as EMAGE and Allen Brain Atlas. The two task forces are working on multiple related project areas, including: •Production of registration fiducials and landmarks •Standards for registration transformations •Data management and handling •Metadata •Provenance •Atlas hub in a Box •WHS for the rat •PONS pan mammalian delineations of WHS dataset These areas are either needed to integrate other components with the INCF DAI, or are integrated with the infrastructure as they are developed. This program welcomes input from the community, and requests expert recommendations in these project areas. Please contact any author for more information. This work was conducted within the WHS and DAI Task Forces of the INCF Program on Digital Brain Atlasing.},
	booktitle = {Society for {Neuroscience}},
	author = {Boline, Jyl and Avants, Brian and Baldock, Richard A. and Bakker, Rembrandt and Burger, Albert and Gee, James and Haselgrove, Christian and Hess, Andreas and Ibanez, Luis and Larson, Stephen D and Majka, Piotr and Okamura-oho, Yuko and Ruffins, Seth and Zaslavsky, Ilya},
	year = {2012},
}

