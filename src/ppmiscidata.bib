	pmid = {20082965},
	keywords = {Animals, Dementia, Humans, Depression, alpha-Synuclein, Anxiety, Autonomic Nervous System Diseases, Lewy Bodies, Parkinson Disease, REM Sleep Behavior Disorder},
	pages = {S1--5},
}

