	keywords = {Aged, Aging, Alzheimer Disease, Atrophy, Cerebrovascular disease, Female, Hippocampus, Humans, Long-term memory, Magnetic Resonance Imaging, Memory Disorders, Memory, Long-Term, Small vessel disease, Temporal Lobe, White Matter},
	pages = {103308},
