	note = {Number: 1
Publisher: Nature Publishing Group},
	keywords = {Brain imaging, Magnetic resonance imaging, Network models, Paediatrics},
	pages = {150003},
	file = {Full Text PDF:/Users/stnava/Zotero/storage/XG5MXK67/Avants et al. - 2015 - The pediatric template of brain perfusion.pdf:application/pdf},
