@inproceedings{kimStructuralConnectivityTensorbased2011,
	title = {Structural connectivity via the tensor-based morphometry},
	isbn = {978-1-4244-4128-0},
	doi = {10.1109/ISBI.2011.5872528},
	abstract = {The tensor-based morphometry (TBM) has been widely used in characterizing tissue volume difference between populations at voxel level. We present a novel computational framework for investigating the white matter connectivity using TBM. Unlike other diffusion tensor imaging (DTI) based white matter connectivity studies, we do not use DTI but only T1-weighted magnetic resonance imaging (MRI). To construct brain network graphs, we have developed a new data-driven approach called the e-neighbor method that does not need any predetermined parcellation. The proposed pipeline is applied in detecting the topological alteration of the white matter connectivity in maltreated children. © 2011 IEEE.},
	booktitle = {Proceedings - {International} {Symposium} on {Biomedical} {Imaging}},
